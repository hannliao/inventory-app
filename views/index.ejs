<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel="stylesheet" href="/styles/index.css" />
  </head>
  <body>
    <table>
      <tr>
        <th>Product</th>
        <th>Name</th>
        <th>Price</th>
        <th>Qty</th>
      </tr>

      <% products.forEach((product) => { %>
      <tr>
        <td><img src="<%= product.src %>" alt="<%= product.name %>" /></td>
        <td><%= product.name %></td>
        <td><%= product.price %></td>
        <td><%= product.qty %></td>
      </tr>
      <% }) %>
    </table>
  </body>
</html>
